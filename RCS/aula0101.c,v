#include<stdio.h>
#define OK 0

int main ()
{
  printf ("int:\n");
  printf ("\t\t%d\n", sizeof (int) );

  printf ("char:\n");
  printf ("\t\t%d\n", sizeof (char) );

  printf ("float:\n");
  printf ("\t\t%d\n", sizeof (float) );

  printf ("double:\n");
  printf ("\t\t%d\n", sizeof (double) );

  printf ("void:\n");
  printf ("\t\t%d\n", sizeof (void) );

  return (OK);
}
